{
  "name": "TanPage",
  "description": "Hello TanPage",
  "version": "1.2.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "pages/popup.html",
    "default_icon": "imgs/extensions.png"
  },
  "icons": {
    "16": "imgs/extensions.png",
    "48": "imgs/extensions.png",
    "128": "imgs/extensions.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "contextMenus",
    "http://*/",
    "https://*/"
  ],
  "chrome_url_overrides": {
    "newtab": "pages/newtab.html"
  }
}